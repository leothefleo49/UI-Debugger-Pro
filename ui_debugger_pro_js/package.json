{
  "name": "ui-debugger-pro",
  "version": "7.5.0",
  "description": "Universal UI Debugger for React & Web Apps. Detect overlaps, cutoffs, alignment issues, and 'ghost' elements.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./next": {
      "types": "./dist/next-plugin.d.ts",
      "import": "./dist/next-plugin.mjs",
      "require": "./dist/next-plugin.js"
    },
    "./webpack": {
      "types": "./dist/webpack-plugin.d.ts",
      "import": "./dist/webpack-plugin.mjs",
      "require": "./dist/webpack-plugin.js"
    },
    "./inject": {
      "import": "./dist/inject.mjs",
      "require": "./dist/inject.js"
    }
  },
  "bin": {
    "ui-debugger-pro": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "keywords": [
    "react",
    "debugger",
    "ui",
    "css",
    "layout",
    "inspector",
    "devtools"
  ],
  "author": "UI Debugger Pro Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vite": ">=4.0.0 || >=5.0.0",
    "webpack": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "webpack": "^5.0.0"
  },
  "files": [
    "dist",
    "bin"
  ]
}
